<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ renderData.title or title or metadata.title }}</title>
  <meta name="description" content="{{ metaDescription or renderData.metaDescription or summary or metadata.description }}">
  <meta name="theme-color" content="{{ styles.colorPrimary }}">
  <link rel="apple-touch-icon" href="/assets/img/apple-touch-icon.jpg">
  <link rel="manifest" href="/static/manifest.json"/>
  {% include "./yandex-metrika.njk" %}
  {% include "./google-analytics.njk" %}
  {% include "assets/css/variables.njk" %}
  {% set css %}
    {% include "assets/css/normalize.css" %}
    {% include "assets/css/base-styles.css" %}
    {% include "assets/css/utilites.css" %}
    {% include "assets/css/site-header.css" %}
    {% include "assets/css/site-footer.css" %}
    {% include "assets/css/blog.css" %}
    {% include "assets/css/projects.css" %}

    {% if page.url == '/' %}
      {% include "assets/css/home-page.css" %}
    {% endif %}

    {% if page.url == '/contact/' %}
      {% include "assets/css/contact.css" %}
    {% endif %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/sw.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
  {% if page.url == '/contact/' %}
  <script src="https://www.google.com/recaptcha/api.js" async defer></script>
  {% endif %}
</head>
